.slide
  h = 20px
  j = round(h * 0.85)
  l = round(h * 0.1)
  transition-duration 100ms
  position absolute
  cursor pointer
  background white
  font-size h
  line-height j
  left 35%
  color #999
  // padding 0 (h / 2)
  opacity 0.5
  width 80%
  left 10%
  text-align center

  &.prev
    top - j
    border-radius l l 0 0
  &.next
    bottom - j
    border-radius 0 0 l l
  &:hover
    opacity 0.9


#steps
  padding 13px 0
  display inline-block
  position absolute
  background url(../vendor/img/line.png) center repeat-x
  .step
