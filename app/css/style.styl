Supra( Colx )
  n = lighten( Colx, 10%) 
  bottomColor = darken(n,20%)
  background-color bottomColor
  background-image linear-gradient(top, n, bottomColor)
  color desaturate(darken(n,70%),10%)
  &.active
    background-image n
    color #222
  // &:active
  //   background darken(n,35%)
  &.inactive
    b = desaturate( darken(n, 10%), 96%)
    background rgba( b, 0.5)
    color desaturate(darken(n,70%),99%)
    &:hover
      background desaturate( darken(n, 10%), 60%)
      color desaturate( darken(n, 75%), 90%)


